<?xml version="1.0" encoding="UTF-8" ?>
<odoo>

    <template id="portal_my_home_sale" name="Check  Order" inherit_id="portal.portal_my_home"
              customize_show="True" priority="30">
        <xpath expr="//div[hasclass('o_portal_docs')]" position="inside">
            <t t-call="portal.portal_docs_entry">
                <t t-set="title">New Custom Portal</t>
                <t t-set="url" t-value="'/my/check_existing_order'"/>
                <t t-set="placeholder_count" t-value="'my_count'"/>
            </t>
        </xpath>
    </template>

</odoo>

